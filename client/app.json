{
  "expo": {
    "name": "routie",
    "slug": "routie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash/Routie_splash_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash/Routie_splash_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF622A"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash/Routie_splash_icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
